<div class= "review-rating" data-score= "<%= review.rating %>"></div>
<p><%= review.comment %></p>

<% if user_signed_in? %>
    <% if review.user_id == current_user.id %>
        <%= link_to "Edit", edit_room_review_path(review.room, review) %>
        <%= link_to "Delete", room_review_path(review.room, review), method: :delete, data: {confirm: "Are you sure?"} %>
   
   <% end %>
 <% end %>
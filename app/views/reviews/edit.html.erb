<h1>Edit Review</h1>

<%= form_for([@room, @review]) do |f| %>
     <div id="rating-form">
       <label>Ratings</label>
     </div>
     
     <div class="field">
        <%= f.label :comment %><br />
        <%= f.text_area :comment %>
     </div>
 
     <div class="field">
        <%= f.submit %>
     </div>
    <% end %>
    
  <script>
 $('#rating-form').raty({
  scoreName: 'review[rating]'
 });
</script>

<%= form_for([@room, @room.reviews.build]) do |f| %>

<div id="rating-form">
  <label>Ratings</label>
</div>

<div class="field">
  <%= f.label :comment %><br />
  <%= f.text_area :comment %>
</div>

<div class="field">
  <%= f.submit %>
</div>
<% end %>

<script>
  $('#rating-form').raty({
     scoreName: 'review[rating]'
   });
   </script>
